CREATE USER <%= @userdb %> IDENTIFIED BY <%= @passdb %> DEFAULT TABLESPACE "USERS" TEMPORARY TABLESPACE "TEMP";

ALTER USER <%= @userdb %> QUOTA UNLIMITED ON USERS;

-- ROLES
GRANT "DBA" TO <%= @userdb %> ;
GRANT "CONNECT" TO <%= @userdb %> ;

-- SYSTEM PRIVILEGES
GRANT CREATE SESSION TO <%= @userdb %>;

exit