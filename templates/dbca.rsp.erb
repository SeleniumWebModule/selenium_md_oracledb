#-----------------------------------------------------------------------------
# GENERAL section is required for all types of database creations.
#-----------------------------------------------------------------------------
[GENERAL]

RESPONSEFILE_VERSION = <%= @oracleVersion %>
OPERATION_TYPE = "createDatabase"

[CREATEDATABASE]

GDBNAME = <%= @dbname %>
SID = <%= @sid %>
TEMPLATENAME = "General_Purpose.dbc"
SYSPASSWORD = <%= @systempass %>
SYSTEMPASSWORD = <%= @systempass %>
DISABLESECURITYCONFIGURATION = "ALL"
CHARACTERSET = "AL32UTF8"
NATIONALCHARACTERSET= "UTF8"
LISTENERS = "listener"

#-----------------------*** End of CREATEDATABASE section 
